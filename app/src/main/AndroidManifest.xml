<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jcs.orderassistant" >

    <application
        android:name=".OrderApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AACountActivity"
            android:label="@string/title_activity_aacount" >
        </activity>
        <activity
            android:name=".AddMemberActivity"
            android:label="@string/title_activity_add_member" >
        </activity>
        <activity
            android:name=".SepcountActivity"
            android:label="@string/title_activity_sepcount" >
        </activity>
        <activity
            android:name=".ReceiveMoneyActivity"
            android:label="@string/title_activity_receive_money" >
        </activity>
    </application>

</manifest>
